package Practice;
public class mock{
	public static void main(String []args)
	{
		int c=0;
		for(int i=5; i>=1; i--)
		{
			for(int j=5; j>=i; j--)
			{
				System.out.print("");
			}
			for(int j=5; j>=i; j--)
			{
				c++;
				System.out.print(c+" ");
			}
			System.out.println(" ");
		}
		
	}
}

//5 4 3 2 1
//4 3 2 1
//3 2 1
//2 1
//1


