{
	int n;
	void print()
	{
		System.out.println("base");
	}
}
class derived extends base
{
	derived()
	{
	super();
	System.out.println("def constr");
	}
	void init(int n)
	{
		super.print();
	}
	void disply(int n)
	{
		System.out.println();
	}
}
