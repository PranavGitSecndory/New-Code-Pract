package Practice;
public class mock{
	public static void main(String []args)
	{
		for(int i=9; i<=5; i++)
		{
			System.out.println("lskdl");
			int temp=i;
			for(int j=i; j<=i; j--)
			{
				System.out.print(temp+" ");
				temp--;
			}
			System.out.println(" ");
		}
	}
}


