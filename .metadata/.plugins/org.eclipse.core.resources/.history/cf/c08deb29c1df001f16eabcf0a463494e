package Practice;
public class mock{
	public static void main(String []args)
	{
		for(int i=5; i>=1; i--)
		{
			for(int j=1; j<=i; j++)
			{
				System.out.print(" ");
			}
			for(int j=5; j>=i; j--)
			{
				System.out.print(i+" ");
			}
			System.out.println(" ");
		}
		
	}
}

//5 4 3 2 1
//4 3 2 1
//3 2 1
//2 1
//1


