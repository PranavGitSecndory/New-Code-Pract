package Practice;
public class mock{
	public static void main(String []args)
	{
		int c=0;
		for(int i=1; i<=5; i++)
		{
			for(int j=5; j>=i; j--)
			{
				System.out.print("");
			}
			for(int j=5; j>=i; j--)
			{
				c++;
				System.out.print(i+" ");
			}
			System.out.println(" ");
		}
		
	}
}



