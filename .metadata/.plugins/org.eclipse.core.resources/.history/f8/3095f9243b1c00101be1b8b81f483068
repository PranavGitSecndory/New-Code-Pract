package Collection;

import java.util.ArrayList;

public class Arraylist_All_Method {

 void add()
{
		ArrayList ob=new ArrayList();
		ob.add("sagar");
		System.out.println("Add method "+ob);
}
 
 void addall()
 {
	 ArrayList ob1=new ArrayList();
	 ArrayList ob2=new ArrayList();
	 ob1.add(1);
	 ob1.add(2);
	 ob1.add(3);
	 
	 ob2.add(4);
	 ob2.add(5);
	 
	 ob2.addAll(ob1);
	 System.out.println("Add all method"+ob2);
 }
public static void main(String[] args)
{
	Arraylist_All_Method ob1=new Arraylist_All_Method();
	ob1.add();
	ob1.addall();
}
}
