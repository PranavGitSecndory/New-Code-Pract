class studentdata{
	int std_id;
	String std_name;
	int mks_csc;
	int mks_mm;
	
	public studentdata(int std_id, String std_name, int mks_csc, int mks_mm) {
		super();
		this.std_id = std_id;
		this.std_name = std_name;
		this.mks_csc = mks_csc;
		this.mks_mm = mks_mm;
	}
	public void showstudentdata()
	{
		System.out.println("student data = "+std_id+" "+std_name+" "+mks_csc+" "+mks_mm);
	}
	void getTomks()
	{
		
	
		
	}

}

@Override
public String toString() {
	return "DemoList [getClass()=" + getClass() + ", hashCode()=" + hashCode() + ", toString()=" + super.toString()
			+ "]";
}
