package File_handling;

import java.io.FileInputStream;
import java.io.ObjectInputStream;
import java.io.Serializable;


public class Deserilization 
{   public static void main(String[] args) {
    try {
        
        FileInputStream ob = new FileInputStream("C:\\\\Users\\\\patil\\\\OneDrive\\\\Desktop\\\\File_handle\\\\divesh.ser");
        ObjectInputStream ob1 = new ObjectInputStream(ob);

        Emp e1 = (Emp) ob1.readObject();
        Emp e2 = (Emp) ob1.readObject();

        System.out.println("ID: " + e1.() + ", Name: " + e1.getName() + ", Address: " + e1.getAddress());
        System.out.println("ID: " + e2.getId() + ", Name: " + e2.getName() + ", Address: " + e2.getAddress());

        ob1.close();
        ob.close();
    } catch (Exception e) {
        System.out.println("Error: " + e);
    }
}


}


	

