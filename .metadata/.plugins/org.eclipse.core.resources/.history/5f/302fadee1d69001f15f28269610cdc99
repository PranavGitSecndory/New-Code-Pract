package Practice;
	
	class base
	{
		int n;
		void print()
		{
			System.out.println("base");
		}
	}
	
	class derived extends base
	{
		void initialize(int a)
		{
			super.n=a;
			System.out.println("a");
		}
		public void display()
		{
			System.out.println("n");
		}
	}
public class superkey_demo {
	
	public static void main(String args[])
	{
		derived ob=new derived();
		ob.initialize(100);
		
	}

}
