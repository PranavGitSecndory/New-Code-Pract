package Collection;

import java.util.ArrayList;

public class Arraylist_All_Method {

 void add()
{
		ArrayList ob=new ArrayList();
		ob.add("sagar");
		System.out.println("Add method "+ob);
}
 
 void addall()
 {
	 ArrayList ob1=new ArrayList();
	 ArrayList ob2=new ArrayList();
	 ob1.add(1);
	 ob1.add(2);
	 ob1.add(3);
	 
	 ob2.add(4);
	 ob2.add(5);
	 
	 ob1.addAll(ob2);
	 System.out.println("Add all method"+ob1);
 }
 
 void remove()
 {
	 ArrayList ob3=new ArrayList();
	 ob3.add("nick");
	 ob3.add("sagar");
	 ob3.add("ganesh");
	 System.out.println("befor remove"+ob3);
	 ob3.remove("ganesh");
	 System.out.println("after remove"+ob3);
	 
 }
 void removeall()
 {
	 ArrayList ob4= new ArrayList();
	 ob4.add("nick");
	 ob4.add("sagar");
	 ob4.add("ganesh");
	 System.out.println("Befor remove all"+ob4);
	 ob4.removeAll(ob4);
	 System.out.println("After remove all"+ob4);
 }
public static void main(String[] args)
{
	Arraylist_All_Method ob1=new Arraylist_All_Method();
	ob1.add();
	ob1.addall();
	ob1.remove();
	ob1.removeall();
}
}
