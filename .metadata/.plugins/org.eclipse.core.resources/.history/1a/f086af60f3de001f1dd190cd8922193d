package Practice;
import java.util.Scanner;
public class mock
{
	 public static void main(String[] args) {
	        Scanner scanner = new Scanner(System.in);
	        System.out.print("Enter a number: ");
	        int number = scanner.nextInt();
	        
	        int product = 1;
	        boolean hasNonZeroDigits = false;
	        
	        while (number > 0) {
	            int digit = number % 10;
	            if (digit != 0) {
	                product *= digit;
	                hasNonZeroDigits = true;
	            }
	            number /= 10;
	        }
	        
	        if (hasNonZeroDigits) {
	            System.out.println("Product of non-zero digits: " + product);
	        } else {
	            System.out.println("No non-zero digits in the number.");
	        }
	    }
}