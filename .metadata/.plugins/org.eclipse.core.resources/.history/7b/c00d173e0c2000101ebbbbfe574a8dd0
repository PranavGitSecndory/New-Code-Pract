package syncronization;

public class Q1_A extends Thread{
	public void run()
	{
		for(int i=0; i<=5; i++)
		{
			synchronized (this)
			{
				System.out.println("number"+i);
				try {
					Thread.sleep(1000);
				} catch (Exception e) {
					System.out.println(e);
				}
			}
		}
	}
public static void main(String[] args) {
	Q1_A ob=new Q1_A();
	ob.start();
}
}
