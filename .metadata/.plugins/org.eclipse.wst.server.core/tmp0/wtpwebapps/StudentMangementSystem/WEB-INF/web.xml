<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

  <display-name>StudentManagementSystem</display-name>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
   <servlet>
    <servlet-name>AddStudentServlet</servlet-name>
    <servlet-class>com.controller.AddStudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddStudentServlet</servlet-name>
    <url-pattern>/AddStudentServlet</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
 
 
 
  <servlet>
    <servlet-name>DeleteStudentServlet</servlet-name>
    <servlet-class>com.controller.DeleteStudentSetvlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteStudentServlet</servlet-name>
    <url-pattern>/DeleteStudentServlet</url-pattern>
  </servlet-mapping>

  
  <servlet>
    <servlet-name>UpdateStudentServlet</servlet-name>
    <servlet-class>com.controller.UpdateStudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateStudentServlet</servlet-name>
    <url-pattern>/UpdateStudentServlet</url-pattern>
  </servlet-mapping>





 
<servlet>
  <servlet-name>ViewStudents</servlet-name>
  <servlet-class>com.controller.ViewStudentsServlet</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>ViewStudents</servlet-name>
  <url-pattern>/view</url-pattern>
</servlet-mapping>
 
 


</web-app>
